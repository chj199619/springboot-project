TB.namespace("TB.AccountManage");TB.AccountManage=function(){var d=YAHOO.util,c=d.Dom,b=d.Event,a=d.Lang;return{togglePanel:function(f,e,g){f=c.get(f);e=c.get(e);if(!f||!e){return}b.on(f,"click",function(i){b.preventDefault(i);var h=(e.style.display!="none");e.style.display=h?"none":"block";if(g){g(f,e)}})},initTogglePanel:function(f,e){TB.AccountManage.togglePanel(f,e,function(g){if(c.hasClass(g,"collapsed-tool-toggle")){c.removeClass(g,"collapsed-tool-toggle")}else{c.addClass(g,"collapsed-tool-toggle")}})}}}();
